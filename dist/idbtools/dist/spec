product noship
	id "Unshipped Software PD, SGIUseOnly, 4D1-2.0 000-0000-000"
	image misc version 0000000${ALPHA}
		id "Unshipped SW, 4D1-2.0 000-0000-000"
		subsys pas
			id "Pascal Compiler"
			exp "pas.sw.pas"
			replaces self
			.
		subsys other
			id "other."
			exp "noship && !std.sw.mas1 && !pas.sw.pas"
			replaces self
			.
		subsys mas1
			id "graphics master spin 1"
			exp "std.sw.mas1"
			replaces self
			.
		.
	.
