product std1
	id "Std Sys PD 1, 4D1-2.0 808-0003-003"
	image sw version 0004002${ALPHA}
		id "Std Sys SW 1, 4D1-2.0 808-0004-002"
		order 10
		subsys unix default main
			id "Basic UNIX"
			exp "!noship&&(std.sw.unix||std.unix.lboot||std.sw.scsi||std.sw.xyl714)"
			replaces self
			.
		.
	image man version 0005002${ALPHA}
		id "Std Sys Man 1, 4D1-2.0 808-0005-002"
		order 11
		subsys unix default
			id "Basic UNIX"
			exp "!noship && std.man.unix"
			replaces self
			.
		.
	.

product std2
	id "Std Sys PD 2, 4D1-2.0 808-0028-002"
